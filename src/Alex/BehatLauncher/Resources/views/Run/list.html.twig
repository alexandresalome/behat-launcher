{% extends "layout.html.twig" %}

{% set title = 'title.run_list'|transchoice(runs|length, {'%count%': runs|length}) %}
{% block title title %}

{% set menu_active = "run_list" %}
{% set refresh = true %}

{% block content %}
    <h1>{{ title }}</h1>
    {{ include('Run/_list.html.twig') }}
{% endblock %}

{% block angular %}
{% verbatim %}
    <h1>Last {{ runs.length }} runs</h1>
    <div ng-include="'/templates/_run_list.html'" />
{% endverbatim %}
{% endblock %}
